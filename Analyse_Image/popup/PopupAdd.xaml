<Window x:Class="Analyse_Image.popup.PopupAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Analyse_Image.popup"
        mc:Ignorable="d"
        Title="Selectionnez une image" Height="164" Width="320">
    <Grid>
        <TextBox x:Name="fileNameTextBox" IsReadOnly="True" HorizontalAlignment="Left" Margin="78,53,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="215" Height="18"/>
        <Label Content="Selectionner une image" HorizontalAlignment="Left" Margin="14,19,0,0" VerticalAlignment="Top"/>
        <Button Click="Done" Content="Continuer" HorizontalAlignment="Left" Margin="78,90,0,0" VerticalAlignment="Top"/>
        <Button Click="Cancel" Content="Annuler" HorizontalAlignment="Left" Margin="160,90,0,0" VerticalAlignment="Top"/>
        <Button Click="Select" Content="Selection" HorizontalAlignment="Left" Margin="14,52,0,0" VerticalAlignment="Top" Height="19" Width="52"/>
    </Grid>
</Window>
